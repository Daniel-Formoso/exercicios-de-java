package entities;

public class Conta {

	public int numConta;
	public String nomeCliente;
	public double valorDepositoInicial;
	
	public Conta() {
		
	}
	
	public Conta(int numConta, String nomeCliente, double valorDepositoInicial) {
		super();
		this.numConta = numConta;
		this.nomeCliente = nomeCliente;
		this.valorDepositoInicial = valorDepositoInicial;
	}

	public String getNomeCliente() {
		return nomeCliente;
	}

	public void setNomeCliente(String nomeCliente) {
		this.nomeCliente = nomeCliente;
	}

	public double getValorDepositoInicial() {
		return valorDepositoInicial;
	}

	public void setValorDepositoInicial(double valorDepositoInicial) {
		this.valorDepositoInicial = valorDepositoInicial;
	}

	public double deposito() {
