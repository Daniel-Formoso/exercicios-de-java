		System.out.print("Quantos funcionários serão registrados? ");
		int n = sc.nextInt();
		
		List<Employees> list = new ArrayList<>();
				
		for (int i = 0; i < n; i++) {
			System.out.print(" id : ");
		}
		
		
		sc.close();
		
	}

}
